{
    "name": "floo/dx-adapter",
    "description": "Query builder for Laravel model, specifically for DevExtream Table",
    "keywords": [
        "floo",
        "query",
        "dx-adapter",
        "query-builder"
    ],
    "homepage": "https://github.com/floo/dx-adapter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yunus Floo",
            "email": "yunus@floo.id",
            "role": "Developer"
        },
        {
            "name": "Ghoni Jee",
            "email": "akughoni@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "ergebnis/phpstan-rules": "^0.15.3",
        "pestphp/pest": "^1.20.0",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-strict-rules": "^0.12.11",
        "styleci/cli": "^1.2",
        "thecodingmachine/phpstan-strict-rules": "^0.12.1",
        "orchestra/testbench": "^6.0",
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Floo\\DxAdapter\\": "src",
            "Floo\\DxAdapter\\Database\\Factories\\": "Database/Factories",
            "Floo\\DxAdapter\\Models\\": "Models"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Floo\\DxAdapter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Floo\\DxAdapter\\DxAdapterServiceProvider"
            ],
            "aliases": {
                "DxAdapter": "Floo\\DxAdapter\\DxAdapterFacade"
            }
        }
    }
}
