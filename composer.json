{
    "name": "floo/dx-adapter",
    "description": "Query builder for DevExtream Table",
    "keywords": [
        "floo",
        "dx-adapter"
    ],
    "homepage": "https://github.com/floo/dx-adapter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yunus Floo",
            "email": "yunus@floo.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Floo\\DxAdapter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Floo\\DxAdapter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Floo\\DxAdapter\\DxAdapterServiceProvider"
            ],
            "aliases": {
                "DxAdapter": "Floo\\DxAdapter\\DxAdapterFacade"
            }
        }
    }
}
